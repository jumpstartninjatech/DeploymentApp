<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Microsoft Deployment</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <link rel="apple-touch-icon" sizes="76x76" href="~/assets/img/apple-icon.png" />
    <link rel="icon" type="image/png" href="~/assets/img/favicon.png" />

    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />

    <!-- CSS Files -->
    <link href="@Url.Content("~/assets/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/material-bootstrap-wizard.css")" rel="stylesheet" type="text/css" />

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="@Url.Content("~/assets/css/demo.css")" rel="stylesheet" type="text/css" />

</head>

<body>
    <style>
        .image-container {
            background-image: url('../../assets/img/BG2.jpg');
            margin: 0;
            background-repeat: no-repeat;
            background-size: cover;
        }

        h4 {
            color: #F25022;
        }
    </style>
    <div class="image-container set-full-height">
        <!--   Big container   -->
        <div class="container">
            <div class="row">
                <div class="col-sm-8 col-sm-offset-1">
                    <!--      Wizard container        -->
                    <div class="wizard-container" style="width:895px;">
                        <div class="card wizard-card" data-color="red" id="wizard">
                            <div class="wizard-header">
                                <a href="https://www.microsoft.com">
                                    <img src="~/assets/img/new_logo2.jpg" style="width: 16%;float:left;margin-left:40px;margin-top:-13px;">
                                </a>
                                <h3 class="wizard-title" style="color:#00A4EF;text-align:left;">
                                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Sentiment Data Pipeline
                                </h3>
                            </div>
                            <div class="wizard-navigation">
                                <ul>
                                    <li><a href="#details" data-toggle="tab" aria-expanded="false">Azure Source</a></li>
                                    <li class="active"><a href="#description" data-toggle="tab" aria-expanded="true">Summary</a></li>
                                </ul>
                            </div>

                            <div class="tab-content">

                                <div class="tab-pane" id="details" style="display:none;">
                                </div>

                                <div class="tab-pane" id="description" style="display:block;">
                                    <div>
                                        <center>
                                            <h3><b>Deployment is in progress!!!</b></h3>
                                            <h3>It might take few minutes, Please check your Azure Account Portal. </h3>
                                            <h4><b>Resources will be deployed in "ARM-DeployRG" Resource Group...</b></h4>
                                            <h4><b>Database Name : "bingNews"</b></h4>
                                            <i class="material-icons" style="font-size:100px;">
                                                backup
                                            </i>
                                        </center>
                                        </div>
                                    <br />
                                    <br />
                                    <br />
                                    <div>
                                        <input type='button' name='home' value='Home' style="float:left;background:#00A4EF;padding:10px;border-radius:18px;color:white;font-size:18px;width:14%;border-style:unset;" onclick="window.location.href= '/Home/Index';" />
                                    </div>
                                </div>

                            </div>
                            <div class="wizard-footer">
                                <div class="pull-left">
                                    <div class="footer-checkbox">
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                            </div>

                        </div>
                    </div> <!-- wizard container -->
                </div>
            </div> <!-- row -->
        </div> <!--  big container -->
        <!--  -->
    </div>

</body>
<!--   Core JS Files   -->
<script src="~/assets/js/jquery-2.2.4.min.js" type="text/javascript"></script>
<script src="~/assets/js/bootstrap.min.js" type="text/javascript"></script>
<script src="~/assets/js/jquery.bootstrap.js" type="text/javascript"></script>

<!--  Plugin for the Wizard -->
<script src="~/assets/js/material-bootstrap-wizard.js"></script>

<!--  More information about jquery.validate here: http://jqueryvalidation.org/	 -->
<script src="~/assets/js/jquery.validate.min.js"></script>

</html>





