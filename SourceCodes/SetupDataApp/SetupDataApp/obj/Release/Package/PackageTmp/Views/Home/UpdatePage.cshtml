<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Sentiment Data Pipeline</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="author" content="colorlib.com">

    <!-- MATERIAL DESIGN ICONIC FONT -->
    <link rel="stylesheet" href="~/assets/fonts/material-design-iconic-font/css/material-design-iconic-font.css">

    <!-- DATE-PICKER -->
    <link rel="stylesheet" href="~/assets/vendor/date-picker/css/datepicker.min.css">

    <!-- STYLE CSS -->
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="wrapper" style="background: white;border-radius: 5px;padding-top:20px;padding-left:30px;padding-right:30px;padding-bottom:30px;border-color: 7px solid #88893;width: 900px;height: 700px;">
        <form action="" id="wizard">
            <!-- SECTION 1 -->
            <h2 style="color: #00A4EF;font-weight: bold;text-align: center">Sentiment Data Pipeline</h2><br>
            <h5 style="color: #00A4EF;font-weight: bold;text-align: center;font-size: 18px;">Oragnization Settings</h5><br>
            <section>
                <div class="row">
                    <div class="col-lg-11 col-sm-11">
                        <input type="text" class="form-control" placeholder="Search..." style="float: left;">

                    </div>
                    <div class="col-lg-1 col-sm-1">
                        <button class="btn btn-default" type="button" style="border:none;">
                            <i class="fa fa-search" style="height: 11px;background: #ffffff;font-size: 30px;margin-top:-5px;margin-left: -35px;color: #888893"></i>
                        </button>
                    </div>
                </div>
                <h3></h3>
                <div class="row">
                    <div class="col-lg-1 col-sm-1">
                    </div>
                    @model List<SetupDataApp.Models.MySettings>
                    @foreach (var settings in Model)
                    {
                        <div class="col-lg-3 col-sm-3">
                            <img src=@settings.Brand_image_URL alt="Desert" class="img-responsive" style="min-width: 170px;max-width: 170px;padding-top: 25px;">
                        </div>
                        <div class="col-lg-6 col-sm-6">
                            <h1 style="font-weight: bold;">@settings.Organization_Name</h1>
                            <p>@settings.AreaOfConcern</p>
                            <p>@settings.Keywords</p>
                            <p>@settings.Region</p>
                        </div>
                        <div class="col-lg-1 col-sm-1">
                            <button type="button" class="btn btn-primary" style="float:left;" onclick="window.location.href= '/Home/SettingsPage';"><i class="fa fa-angle-double-right" style="padding-top: 0px;font-size: 22px;"></i></button>
                        </div>
                    }
                </div>
                <br><br>
                <button type="button" class="btn btn-primary" style="float:left;">Reset Data</button>
                <button type="button" class="btn btn-primary" style="float:right;" onclick="window.location.href= '/Home/ReportPage';">View Report</button>


            </section>


        </form>
    </div>

    <script src="~/assets/js/jquery-3.3.1.min.js"></script>

    <!-- JQUERY STEP -->
    <script src="~/assets/js/jquery.steps.js"></script>

    <!-- DATE-PICKER -->
    <script src="~/assets/vendor/date-picker/js/datepicker.js"></script>
    <script src="~/assets/vendor/date-picker/js/datepicker.en.js"></script>

    <script src="~/assets/js/main.js"></script>

</body>
</html>